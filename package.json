{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write src//**/*.{ts,tsx}",
    "build:prod": "npm run opennext:build && wrangler deploy --dry-run --outdir=dist",
    "opennext:build": "export SKIP_LINTER=false && opennextjs-cloudflare",
    "build:analyze": "cross-env ANALYZE=true next build",
    "deploy": "npm run opennext:build && wrangler deploy",
    "preview": "npm run opennext:build && wrangler dev",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "get-db-name": "node scripts/get-db-name.mjs",
    "db:migrate:dev": "bash -c \"wrangler d1 migrations apply $(node scripts/get-db-name.mjs) --local\""
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.1",
    "@marsidev/react-turnstile": "^1.1.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-email/components": "^0.0.33",
    "@t3-oss/env-nextjs": "^0.12.0",
    "argon2": "^0.41.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.39.3",
    "lucide-react": "^0.475.0",
    "next": "15.2.0-canary.67",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "resend": "^4.1.2",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2",
    "zsa": "^0.6.0",
    "zsa-react": "^0.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@next/bundle-analyzer": "^15.1.7",
    "@opennextjs/cloudflare": "^0.5.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9",
    "eslint-config-next": "15.2.0-canary.67",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "^5"
  }
}
